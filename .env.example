# Network Configuration
RPC_URL=https://mainnet.hashkey.com                     # Hashkey Chain RPC endpoint

# Deployment Keys
DEPLOYER_PRIVATE_KEY=your_private_key_here              # Deployer wallet private key
DEPLOYER_ADDRESS=your_admin_address_here                # Market admin address
CONTRACT_ADDRESS=nft_collection_address_here            # NFT collection address 

# NFT Market Configuration
ADMIN_MAINNET=admin_mainnet_address_here                # Admin address on mainnet
ADMIN_TESTNET=admin_testnet_address_here                # Admin address on testnet
TREASURY_MAINNET=treasury_mainnet_address_here          # Treasury wallet address for fee collection on mainnet
TREASURY_TESTNET=treasury_testnet_address_here          # Treasury wallet address for fee collection on testnet
WHSK_MAINNET=whsk_mainnet_address_here                  # WHSK address on mainnet
WHSK_TESTNET=whsk_testnet_address_here                  # WHSK address on testnet
MINIMUM_ASK_PRICE=minimum_ask_price_here                # Minimum listing price e.g. "10000000000000000" (0.01 HSK)
MAXIMUM_ASK_PRICE=maximum_ask_price_here                # Maximum listing price e.g. "100000000000000000000000000" (100,000,000 HSK)
PROXY_MAINNET=proxy_mainnet_address_here                # Proxy address on mainnet
PROXY_TESTNET=proxy_testnet_address_here                # Proxy address on testnet
PROXY_ADMIN_MAINNET=proxy_admin_mainnet_address_here    # Proxy admin address on mainnet
PROXY_ADMIN_TESTNET=proxy_admin_testnet_address_here    # Proxy admin address on testnet

# Etherscan API Key
ETHERSCAN_API_KEY=your_etherscan_api_key_here           # Etherscan API key
NETWORK=hashkey_mainnet                                 # Network name (hashkey_mainnet or hashkey_testnet)

# Test Configuration (Optional)
FORK_BLOCK_NUMBER=1234567                               # Block number for forking mainnet
REPORT_GAS=true                                         # Enable gas reporting in tests 